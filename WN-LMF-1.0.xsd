   <xsd:attribute name='status' type='xsd:string' use='optional'/>
   <xsd:attribute name='note' type='xsd:string' use='optional'/>
   <xsd:attribute name='confidenceScore' type='xsd:float' use='optional'/>
  </xsd:complexType>
 </xsd:element>

